{
  "name": "media-tool",
  "version": "1.0.2",
  "description": "火花思维-图片处理工具集",
  "scripts": {
    "build:web": "cross-env NODE_ENV=production node webpack-build.js",
    "build:mac": "electron-builder --dir --x64 --mac",
    "build:linux": "electron-builder --dir --x64 --linux",
    "build:win": "electron-builder --dir --x64 --win && ./fix_win32.sh",
    "lint": "eslint 'src/**/*.js'",
    "pack:linux": "electron-builder --x64 --linux",
    "pack:mac": "electron-builder --x64 --mac",
    "pre-commit": "lint-staged -d",
    "start": "cross-env NODE_ENV=development electron .",
    "test": "jest --detectOpenHandles",
    "upgrade": "yarn upgrade-interactive --latest"
  },
  "author": "duzefeng",
  "license": "ISC",
  "main": "main.js",
  "build": {
    "appId": "cn.huohua.mediaTool",
    "asar": true,
    "asarUnpack": [
      "./dist",
      "./package.json"
    ],
    "directories": {
      "output": "release"
    },
    "files": [
      "main.js",
      "config.js",
      "dist/",
      "!node_modules/ffmpeg-static/**/*",
      "node_modules/ffmpeg-static/ffmpeg*",
      "!node_modules/jpegtran-bin/**/*",
      "node_modules/jpegtran-bin/vendor/*",
      "!node_modules/pngquant-bin/**/*",
      "node_modules/pngquant-bin/vendor/*",
      "package.js"
    ],
    "linux": {
      "category": "",
      "target": [
        "AppImage"
      ]
    },
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "win": {
      "target": "portable"
    },
    "electronDownload": {
      "strictSSL": false
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@hot-loader/react-dom": "^16.13.0",
    "babel-7-jest": "^21.3.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.12.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "electron": "8.2.5",
    "electron-builder": "22.6.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "express": "^4.17.1",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "less": "^3.10.3",
    "less-loader": "^6.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^4.0.4",
    "prettier": "^2.0.5",
    "react-hot-loader": "^4.12.21",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "@huohua/upload-web-qiniu": "^1.0.2",
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "clsx": "^1.1.1",
    "fluent-ffmpeg": "^2.1.2",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-pngquant": "^9.0.1",
    "jpegtran-bin": "^5.0.2",
    "optipng-bin": "^7.0.0",
    "pngquant-bin": "^6.0.0",
    "qiniu": "^7.3.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "uuid": "^8.3.2"
  }
}
